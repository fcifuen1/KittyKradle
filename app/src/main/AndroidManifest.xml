<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="cmsc491.kittykradle">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/kk_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/kk_logo_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".WelcomeScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LoginActivity" />
        <activity android:name=".RegistrationActivity" />
        <activity
            android:name=".SidebarActivity"
            android:label="@string/title_activity_sidebar"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".CatProfileActivity" />
        <activity android:name=".SearchResultActivity" />
        <activity android:name=".RequestFormActivity" />
        <activity
            android:name=".FAQActivity"
            android:label="@string/title_activity_faq" />
        <activity android:name=".SearchActivity" />
        <activity
            android:name=".ContactActivity"
            android:label="@string/title_activity_contact" />
        <activity android:name=".FavoritesActivity" />
        <activity android:name=".Homepage" />
        <activity android:name=".Settings"></activity>
    </application>

</manifest>